<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapepr 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="clMapper">

	<!-- 메인페이지 불러오기 -->
	<select id="selectListCount" resultType="_int">
		SELECT
				COUNT(*)
		  FROM 
		  		COFFEECLASS
		 WHERE
		 		STATUS='Y'
	</select>
	
	 
</mapper>